package com.sysnet.poc.partymapping;

/**
 * 定义了在映射文件中使用到的FieldType常量
 * 
 * @author <a href=huang_y@sysnet.com.cn>huang_y</a>
 * @version 1.0
 */
public class FieldType {
	/**
	 * 字符串类型。char、varchar使用此类型
	 */
	public static final String FIELDTYPE_STRING = "string";
	/**
	 * 整型。
	 */
	public static final String FIELDTYPE_INT = "int";
	/**
	 * 日期型
	 */
	public static final String FIELDTYPE_DATE = "date";
	/**
	 * 浮点型
	 */
	public static final String FIELDTYPE_FLOAT = "float";
	/**
	 * 长整形
	 */
	public static final String FIELDTYPE_LONG = "Long";
}
