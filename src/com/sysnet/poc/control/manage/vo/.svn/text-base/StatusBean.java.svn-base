package com.sysnet.poc.control.manage.vo;

public class StatusBean {

	private int BAT_ID = 0;// 批次id

	private int MIN_OBJECT_ID;// 最小对象序号

	private int MAX_OBJECT_ID;// 最大对象序号

	private int RECORD_COUNT;// 记录数

	private String STAGE_DATE_READY_STATUS;// 登场区准备状态

	private String ODS_DATA_DEAL_STATUS;// ods数据处理状态

	private int currentStatus;// 当前状态，0正常处理，1异常处理

	// 建议子线程暂停
	protected boolean suggestStop = false;

	public boolean isSuggestStop() {
		return suggestStop;
	}

	public void setSuggestStop(boolean suggestStop) {
		this.suggestStop = suggestStop;
	}

	public int getCurrentStatus() {
		return currentStatus;
	}

	public void setCurrentStatus(int currentStatus) {
		this.currentStatus = currentStatus;
	}

	public int getBAT_ID() {
		return BAT_ID;
	}

	public void setBAT_ID(int bat_id) {
		BAT_ID = bat_id;
	}

	public int getMIN_OBJECT_ID() {
		return MIN_OBJECT_ID;
	}

	public void setMIN_OBJECT_ID(int min_object_id) {
		MIN_OBJECT_ID = min_object_id;
	}

	public int getMAX_OBJECT_ID() {
		return MAX_OBJECT_ID;
	}

	public void setMAX_OBJECT_ID(int max_object_id) {
		MAX_OBJECT_ID = max_object_id;
	}

	public int getRECORD_COUNT() {
		return RECORD_COUNT;
	}

	public void setRECORD_COUNT(int record_count) {
		RECORD_COUNT = record_count;
	}

	public String getSTAGE_DATE_READY_STATUS() {
		return STAGE_DATE_READY_STATUS;
	}

	public void setSTAGE_DATE_READY_STATUS(String stage_date_ready_status) {
		STAGE_DATE_READY_STATUS = stage_date_ready_status;
	}

	public String getODS_DATA_DEAL_STATUS() {
		return ODS_DATA_DEAL_STATUS;
	}

	public void setODS_DATA_DEAL_STATUS(String ods_data_deal_status) {
		ODS_DATA_DEAL_STATUS = ods_data_deal_status;
	}

}
