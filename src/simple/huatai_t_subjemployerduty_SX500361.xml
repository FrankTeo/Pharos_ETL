				<item fieldName="AggrRscExpnsvCurrency" fieldType="String" parameterType="CSNODE" parameter="SXU00276" method="getElementByCode.getValue.getCurrency" /> <!-- 累计救援费用限额币种 -->
				<item fieldName="AggrRscExpensive" fieldType="Float" parameterType="CSNODE" parameter="SXU00276" method="getElementByCode.getValue.getAmount" /> <!-- 累计救援费用限额 -->
