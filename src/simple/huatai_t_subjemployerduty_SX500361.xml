				<item fieldName="PerPrsnDthLimit" fieldType="Float" parameterType="CSNODE" parameter="SXU00272" method="getElementByCode.getValue.getAmount" /> <!-- 每人死亡责任限额 -->
				<item fieldName="PerPrsnDsblLmtCrrency" fieldType="String" parameterType="CSNODE" parameter="SXU00273" method="getElementByCode.getValue.getCurrency" /> <!-- 每人伤残责任限额币种 -->
				<item fieldName="PerPrsnDsblLimit" fieldType="Float" parameterType="CSNODE" parameter="SXU00273" method="getElementByCode.getValue.getAmount" /> <!-- 每人伤残责任限额 -->
				<item fieldName="PerPrsnMdclExpnsvCurrency" fieldType="String" parameterType="CSNODE" parameter="SXU00274" method="getElementByCode.getValue.getCurrency" /> <!-- 每人医疗费用责任限额币种 -->
